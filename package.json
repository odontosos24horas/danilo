{
  "name": "odontosos",
  "version": "0.10.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "clean": "rm -rf node_modules .next yarn.lock package-lock.json && yarn cache clean",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx",
    "precommit": "yarn lint",
    "commit": "cz",
    "prepush": "git diff HEAD --quiet && git fetch && git pull origin main && yarn precommit && yarn && yarn build",
    "go": "yarn prepush && yarn dev",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.7",
    "@chakra-ui/react": "^1.6.12",
    "@emotion/react": "^11",
